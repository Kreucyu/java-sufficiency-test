package model.entities;

public class Idoso extends Passageiro {
	private String RG;

	public Idoso(String nome, Integer idade, Viagem viagem, String RG) {
		super(nome, idade, viagem);
		this.RG = RG;
	}

	public String getRG() {
		return RG;
	}

	public void setRG(String rG) {
		RG = rG;
	}

	@Override
	public float getTarifa() {
		return 0;
	}

}
